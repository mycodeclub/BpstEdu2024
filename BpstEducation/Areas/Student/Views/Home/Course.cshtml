@model List< BpstEducation.Models.BatchStudent>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_StuLayout.cshtml";
    ViewBag.ActiveTabName = "Helpercourse";
} 
<div class="content">
    <div class="card">
        @foreach (var batch in Model)
        {
            <div class="card-body ">
                <dl class="row">
                    <dt class="col-md-4 col-sm-6 col-lg-3">
                        Course 
                    </dt>
                    <dd class="col-sm-12">
                        @batch.Batch.Course.Name
                    </dd>
                </dl>
            </div>
        }
    </div>
</div>

