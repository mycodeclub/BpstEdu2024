@model BpstEducation.Models.StudentFee

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_StuLayout.cshtml";
    ViewBag.ActiveTabName = "Fee";

}
<h1>Student Fees Details</h1>
<div class="content">
    <div class="card">

        <div class="card-body ">

            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.SubmittedFee)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.SubmittedFee)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FeeSubmittingDate)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FeeSubmittingDate)
                </dd>
            </dl>
        </div>
    </div>
</div> 


