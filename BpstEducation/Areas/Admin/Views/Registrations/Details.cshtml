@model BpstEducation.Models.Registration

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Student Details</h4>
                </div>
                <div class="card-body">
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Status)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Status.RegistrationStatus)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.FullName)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.FullName)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.FatherName)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.FatherName)
                        </dd>
                     
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.CollegeName)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.CollegeName)
                        </dd>
                   
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Qualification)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Qualification)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Course)
                        </dt>
@*                         <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Course.CourseName)
                        </dd> *@
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Message)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Message)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.MobileNumber)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.MobileNumber)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.AlternateMobileNumber)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.AlternateMobileNumber)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.EmailId)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.EmailId)
                        </dd>
                    </dl>
                    <div class="col-12 text-right"><a asp-action="Index" class="btn btn-info">Back</a></div>
                </div>
            </div>
        </div>
    </div>
</div>
